{
  "name": "coin-bbo",
  "version": "0.5.9",
  "description": "A crawler to get realtime BBO messages from crypto exchanges.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "prettier": "prettier -c --write '**/*'",
    "test": "jest --config jest.config.json --no-cache",
    "prepare": "npm run build && npm run lint && npm run prettier && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/coin-bbo.git"
  },
  "keywords": [
    "typescript",
    "CLI"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "soulmachine",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/soulmachine/coin-bbo/issues"
  },
  "homepage": "https://github.com/soulmachine/coin-bbo",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/ws": "^7.2.5",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-each": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "crypto-crawler": "^2.9.6",
    "crypto-markets": "^0.3.2",
    "debug": "^4.1.1",
    "fastpriorityqueue": "^0.6.3",
    "ws": "^7.3.0",
    "yargs": "^15.3.1"
  }
