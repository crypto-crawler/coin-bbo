{
  "name": "coin-bbo",
  "version": "0.4.7",
  "description": "A crawler to get realtime BBO messages from crypto exchanges.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "prettier": "prettier -c --write '**/*'",
    "test": "jest --config jest.config.json --no-cache",
    "prepare": "npm run build && npm run lint && npm run prettier && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/coin-bbo.git"
  },
  "keywords": [
    "typescript",
    "CLI"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "soulmachine",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/soulmachine/coin-bbo/issues"
  },
  "homepage": "https://github.com/soulmachine/coin-bbo",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/ws": "^7.2.3",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-each": "^25.3.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "crypto-crawler": "^2.6.7",
    "crypto-markets": "^0.2.6",
    "debug": "^4.1.1",
    "fastpriorityqueue": "^0.6.3",
    "ws": "^7.2.3",
    "yargs": "^15.3.1"
  }
